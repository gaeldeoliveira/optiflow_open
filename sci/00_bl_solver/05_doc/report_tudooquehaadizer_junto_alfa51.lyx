\begin_layout Subsection
Implicit Solvers
\end_layout

\begin_layout Standard
Implicit solvers solve problems of the type:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
0=f\left(\boldsymbol{y},\frac{\partial\boldsymbol{y}}{\partial\xi},\xi\right)\qquad,\quad\begin{cases}
\boldsymbol{y} & \in\mathbb{R}^{n}\\
\xi & \in\mathbb{R}\\
f & :\mathbb{R}^{n}\times\mathbb{R}\rightarrow\mathbb{R}^{n}
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
with initial conditions provided as input:
\begin_inset Formula 
\[
\begin{cases}
\xi=\xi_{0}\\
\boldsymbol{y}_{0}=\left.\boldsymbol{y}\right|_{\xi=\xi_{0}}\\
\left.\frac{\partial\boldsymbol{y}}{\partial\xi}\right|_{\xi=\xi_{0}}\::\quad f\left(\boldsymbol{y}_{0},\left.\frac{\partial\boldsymbol{y}}{\partial\xi}\right|_{\xi=\xi_{0}},\xi_{0}\right)=0
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
where the initial condition for the derivatives must be consistent and is
 usually be determined numerically.
\end_layout

\begin_layout Subsection
Explicit Solvers
\end_layout

\begin_layout Standard
