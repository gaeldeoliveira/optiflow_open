\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial\boldsymbol{y}}{\partial\xi}=f\left(\boldsymbol{y},\xi\right)\qquad,\quad\begin{cases}
\boldsymbol{y} & \in\mathbb{R}^{n}\\
\xi & \in\mathbb{R}\\
f & :\mathbb{R}^{n}\times\mathbb{R}\rightarrow\mathbb{R}^{n}
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
with initial conditions provided as input:
\begin_inset Formula 
\[
\begin{cases}
\xi=\xi_{0}\\
\boldsymbol{y}_{0}=\left.\boldsymbol{y}\right|_{\xi=\xi_{0}}
\end{cases}
\]

\end_inset


\end_layout

