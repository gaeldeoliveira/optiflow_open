\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial U}{\partial X}+\frac{\partial V}{\partial Y}=0\qquad\Rightarrow\qquad O\left[\frac{\partial U}{\partial X}\right]=O\left[\frac{\partial V}{\partial Y}\right]
\]

\end_inset


\begin_inset Formula 
\[
\qquad\Leftrightarrow\quad\frac{O\left[U\right]}{O\left[X\right]}=\frac{O\left[V\right]}{O\left[Y\right]}\quad\Leftrightarrow\quad\frac{\bar{U}}{L}=\frac{O\left[V\right]}{\delta}\Leftrightarrow\quad\frac{\delta}{L}\bar{U}=O\left[V\right]
\]

\end_inset


\end_layout

\begin_layout Subsubsection
