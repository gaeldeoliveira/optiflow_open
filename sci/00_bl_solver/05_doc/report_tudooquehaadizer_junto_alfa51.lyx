\begin_layout Subsection
Momentum
\end_layout

\begin_layout Standard
Let us now observe the first momentum equation of the Boundary Layer PDEs:
\begin_inset Formula 
\[
U\frac{\partial U}{\partial X}+V\frac{\partial U}{\partial Y}=U_{e}\frac{\partial U_{e}}{\partial X}+\nu\frac{\partial^{2}U}{\partial Y^{2}}+\frac{1}{\rho}F_{x}
\]

\end_inset


\end_layout

\begin_layout Standard
Before we rework it, let us redefine 
\begin_inset Formula $U$
\end_inset

 in terms of 
\begin_inset Formula $g$
\end_inset

:
\begin_inset Formula 
\[
U=U_{e}f_{\left(\eta\right)}=U_{e}\frac{\partial g}{\partial\eta}
\]

\end_inset


\end_layout

\begin_layout Standard
And do the same for its first derivatives:
\begin_inset Formula 
\[
\frac{\partial U}{\partial X}=f_{\left(\eta\right)}\frac{\partial U_{e}}{\partial X}-\eta\frac{\partial f}{\partial\eta}\frac{U_{e}}{\delta}\frac{\partial\delta}{\partial X}=\frac{\partial g}{\partial\eta}\frac{\partial U_{e}}{\partial X}-\eta\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}}{\delta}\frac{\partial\delta}{\partial X}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial U}{\partial Y}=U_{e}\frac{\partial f}{\partial\eta}\frac{\partial\eta}{\partial Y}=\frac{U_{e}}{\delta}\frac{\partial f}{\partial\eta}=\frac{U_{e}}{\delta}\frac{\partial^{2}g}{\partial\eta^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
Before we attack the second y derivative:
\begin_inset Formula 
\[
\frac{\partial^{2}U}{\partial Y^{2}}=\frac{\partial}{\partial Y}\left(\frac{U_{e}}{\delta}\frac{\partial^{2}g}{\partial\eta^{2}}\right)=\frac{U_{e}}{\delta}\frac{\partial}{\partial Y}\left(\frac{\partial^{2}g}{\partial\eta^{2}}\right)+\frac{\partial^{2}g}{\partial\eta^{2}}\frac{\partial}{\partial Y}\left(\frac{U_{e}}{\delta}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
Recalling that 
\begin_inset Formula $U_{e},\delta\bot y$
\end_inset

, we get:
\begin_inset Formula 
\[
\frac{\partial^{2}U}{\partial Y^{2}}=\frac{U_{e}}{\delta}\frac{\partial}{\partial Y}\left(\frac{\partial^{2}g}{\partial\eta^{2}}\right)=\frac{U_{e}}{\delta}\frac{\partial}{\partial\eta}\left(\frac{\partial^{2}g}{\partial\eta^{2}}\right)\frac{\partial\eta}{\partial Y}\qquad\qquad
\]

\end_inset


\begin_inset Formula 
\[
\qquad\qquad\qquad\qquad\quad=\frac{U_{e}}{\delta}\frac{\partial^{3}g}{\partial\eta^{3}}\frac{\partial\eta}{\partial Y}=\frac{U_{e}}{\delta^{2}}\frac{\partial^{3}g}{\partial\eta^{3}}
\]

\end_inset


\end_layout

\begin_layout Standard
So that we can now rework the momentum equation term by term! We start with
 the first convective term:
\begin_inset Note Comment
status collapsed

\begin_layout Plain Layout
\begin_inset Formula 
\[
U\frac{\partial U}{\partial X}+V\frac{\partial U}{\partial Y}=U_{e}\frac{\partial U_{e}}{\partial X}+\nu\frac{\partial^{2}U}{\partial Y^{2}}+\frac{1}{\rho}F_{x}
\]

\end_inset


\begin_inset Formula 
\[
\left(U_{e}\frac{\partial g}{\partial\eta}\right)\left(\frac{\partial g}{\partial\eta}\frac{\partial U_{e}}{\partial X}-\eta\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}}{\delta}\frac{\partial\delta}{\partial X}\right)+\left(U_{e}\frac{\partial\delta}{\partial X}\eta\frac{\partial g}{\partial\eta}-\frac{\partial}{\partial X}\left(\delta U_{e}\right)g_{\left(\eta\right)}+C_{\eta}\right)\left(\frac{U_{e}}{\delta}\frac{\partial^{2}g}{\partial\eta^{2}}\right)
\]

\end_inset


\end_layout

\end_inset


\begin_inset Formula 
\[
U\frac{\partial U}{\partial X}=\left(U_{e}\frac{\partial g}{\partial\eta}\right)\left(\frac{\partial g}{\partial\eta}\frac{\partial U_{e}}{\partial X}-\eta\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}}{\delta}\frac{\partial\delta}{\partial X}\right)=U_{e}\left(\frac{\partial g}{\partial\eta}\right)^{2}\frac{\partial U_{e}}{\partial X}-\eta\frac{\partial g}{\partial\eta}\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}^{2}}{\delta}\frac{\partial\delta}{\partial X}
\]

\end_inset


\end_layout

\begin_layout Standard
And proceed to the second convective term:
\begin_inset Formula 
\[
V\frac{\partial U}{\partial Y}=\left(U_{e}\frac{\partial\delta}{\partial X}\eta\frac{\partial g}{\partial\eta}-\frac{\partial}{\partial X}\left(\delta U_{e}\right)g_{\left(\eta\right)}+C_{\eta}\right)\left(\frac{U_{e}}{\delta}\frac{\partial^{2}g}{\partial\eta^{2}}\right)=\qquad\qquad
\]

\end_inset


\begin_inset Formula 
\[
\qquad\qquad=\eta\frac{\partial g}{\partial\eta}\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}^{2}}{\delta}\frac{\partial\delta}{\partial X}-g_{\left(\eta\right)}\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}}{\delta}\frac{\partial}{\partial X}\left(\delta U_{e}\right)+\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}}{\delta}C_{\eta}
\]

\end_inset

So that the two convective terms turn into:
\begin_inset Formula 
\[
U\frac{\partial U}{\partial X}+V\frac{\partial U}{\partial Y}=
\]

\end_inset


\begin_inset Formula 
\[
=U_{e}\left(\frac{\partial g}{\partial\eta}\right)^{2}\frac{\partial U_{e}}{\partial X}\underbrace{-\eta\frac{\partial g}{\partial\eta}\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}^{2}}{\delta}\frac{\partial\delta}{\partial X}+\eta\frac{\partial g}{\partial\eta}\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}^{2}}{\delta}\frac{\partial\delta}{\partial X}}_{=0}-g_{\left(\eta\right)}\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}}{\delta}\frac{\partial}{\partial X}\left(\delta U_{e}\right)+\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}}{\delta}C_{\eta}
\]

\end_inset


\begin_inset Formula 
\[
=\left(\frac{\partial g}{\partial\eta}\right)^{2}U_{e}\frac{\partial U_{e}}{\partial X}-g_{\left(\eta\right)}\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}}{\delta}\frac{\partial}{\partial X}\left(\delta U_{e}\right)+\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}}{\delta}C_{\eta}
\]

\end_inset


\end_layout

\begin_layout Standard
So we can finally write the complete momentum equation:
\begin_inset Formula 
\[
U\frac{\partial U}{\partial X}+V\frac{\partial U}{\partial Y}=U_{e}\frac{\partial U_{e}}{\partial X}+\nu\frac{\partial^{2}U}{\partial Y^{2}}+\frac{1}{\rho}F_{x}\qquad\qquad\qquad
\]

\end_inset


\begin_inset Formula 
\[
\Leftrightarrow\qquad\left(\frac{\partial g}{\partial\eta}\right)^{2}U_{e}\frac{\partial U_{e}}{\partial X}-g_{\left(\eta\right)}\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}}{\delta}\frac{\partial}{\partial X}\left(\delta U_{e}\right)+\frac{\partial^{2}g}{\partial\eta^{2}}\frac{U_{e}}{\delta}C_{\eta}=\qquad
\]

\end_inset


\begin_inset Formula 
\[
\qquad\qquad\qquad\qquad\qquad\qquad\qquad\qquad=U_{e}\frac{\partial U_{e}}{\partial X}+\nu\frac{U_{e}}{\delta^{2}}\frac{\partial^{3}g}{\partial\eta^{3}}+\frac{1}{\rho}F_{x}
\]

\end_inset


\end_layout

